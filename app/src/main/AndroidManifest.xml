<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cisoft.lazyorder">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.cisoft.lazyorder.WelcomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name=".ui.shop.ShopActivity"
            android:label="@string/title_activity_shop"></activity>
        <activity
            android:name="com.cisoft.lazyorder.ui.goods.GoodsActivity"
            android:label="@string/title_activity_goods"
            android:parentActivityName="com.cisoft.lazyorder.ui.shop.ShopActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cisoft.lazyorder.ui.shop.ShopActivity" />
        </activity>

        <activity android:name=".ui.search.SearchActivity"
            android:label="@string/title_activity_search"/>
    </application>

</manifest>
