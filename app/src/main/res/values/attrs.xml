<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <attr name="autoPlay" forMymat="boolean" />
    <attr name="changeTimeInterval" format="integer" />
    <attr name="count" format="integer" />
    <declare-styleable name="adRotatorAttrs">
        <attr name="autoPlay" />
        <attr name="changeTimeInterval" />
        <attr name="count" />
    </declare-styleable>

    <attr name="menu_name" format="string" />
    <attr name="menu_icon" format="reference|integer" />
    <attr name="content_class" format="string" />
    <declare-styleable name="drawMenuItemAttrs">
        <attr name="menu_name" />
        <attr name="menu_icon" />
        <attr name="content_class" />
    </declare-styleable>

    <attr name="item_title" format="string" />
    <attr name="item_icon" format="reference|integer" />
    <declare-styleable name="userCenterItemAttrs">
        <attr name="item_title" />
        <attr name="item_icon" />
    </declare-styleable>
</resources>