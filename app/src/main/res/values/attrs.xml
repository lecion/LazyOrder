<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <attr name="autoPlay" forMymat="boolean" />
    <attr name="changeTimeInterval" format="integer" />
    <attr name="count" format="integer" />
    <declare-styleable name="adRotatorAttrs">
        <attr name="autoPlay" />
        <attr name="changeTimeInterval" />
        <attr name="count" />
    </declare-styleable>

    <attr name="menu_name" format="string" />
    <attr name="menu_icon" format="reference|integer" />
    <attr name="content_class" format="string" />
    <attr name="need_login" format="boolean" />
    <declare-styleable name="drawMenuItemAttrs">
        <attr name="menu_name" />
        <attr name="menu_icon" />
        <attr name="content_class" />
        <attr name="need_login" />
    </declare-styleable>

    <attr name="item_title" format="string" />
    <attr name="item_icon" format="reference|integer" />
    <declare-styleable name="userCenterItemAttrs">
        <attr name="item_title" />
        <attr name="item_icon" />
    </declare-styleable>


    <attr name="name" format="reference|string" />
    <attr name="icon" format="reference" />
    <declare-styleable name="SectionItemView">
        <attr name="name" />
        <attr name="icon" />
    </declare-styleable>

    <attr name="headerText" format="string" />
    <attr name="footerText" format="string" />
    <declare-styleable name="SectionView">
        <attr name="headerText" />
        <attr name="footerText" />
    </declare-styleable>
</resources>